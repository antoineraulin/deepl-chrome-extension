{
    "manifest_version": 2,
  
    "name": "DeepL",
    "description": "DeepL extension pour Chrome",
    "version": "1.0",
    "icons": { "16": "icon16.png",
      "48": "icon48.png",
     "128": "icon128.png" },
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab",
      "https://www.deepl.com/jsonrpc"
     ]
  }